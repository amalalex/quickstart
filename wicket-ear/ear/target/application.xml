<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_6.xsd" version="6">
  <description>A sample JAVA EE 7 project demonstrating how to use Wicket Framework with JBoss WildFly.
        EAR with EJB JAR and WAR version.</description>
  <display-name>wildfly-wicket-ear-ear</display-name>
  <module>
    <ejb>wildfly-wicket-ear-ejb.jar</ejb>
  </module>
  <module>
    <web>
      <web-uri>wildfly-wicket-ear-war.war</web-uri>
      <context-root>/wildfly-wicket-ear-war</context-root>
    </web>
  </module>
  <library-directory>lib</library-directory>
</application>